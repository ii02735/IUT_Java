package état;

import appli.État;

public class Payée implements État{

	@Override
	public État payer() throws Exception{
		// TODO Auto-generated method stub
		throw new Exception("Une commande payée ne peut être payée une seconde fois");
		
	}

	@Override
	public État rembourser() throws Exception{
		// TODO Auto-generated method stub
		return new NonPayée();
		
	}

	@Override
	public État livrer() throws Exception{
		// TODO Auto-generated method stub
		return new Livrée();
		
	}

}
