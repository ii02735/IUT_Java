package fileSystem;

public class File {
	private String name;
	public File(String name) {
		this.name = name;
	}

	public String display(String path) {
		return (path + name + "\n");
	}// println --> attention : on l'utilise pour le débogage ou le main mais
		// pas dans les classes

	public String ls() {
		return display("");
	}

	public String getName()
	{
		return this.name;
	}

	
}